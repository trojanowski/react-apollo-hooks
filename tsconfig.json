{
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es2015", "esnext.asynciterable"],

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
